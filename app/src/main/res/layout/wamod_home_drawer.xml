<?xml version="1.0" encoding="utf-8"?>
<merge
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <!--<LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/wamod_drawer_container">
        <com.wamod.NavigationDrawer
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@android:color/transparent">-->
            <RelativeLayout
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="250dp"
                android:layout_height="match_parent"
                android:background="#404040"
                android:id="@+id/wamod_drawer">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:clickable="true">
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="180dp"
                        android:id="@+id/wamod_drawer_header">
                        <ImageView
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:src="@drawable/wamod_drawer_bg"
                            android:scaleType="centerCrop"
                            android:id="@+id/wamod_drawer_bgview"/>
                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentBottom="true"
                            android:layout_margin="15dp">
                            <com.pkmmte.view.CircularImageView
                                android:layout_width="50dp"
                                android:layout_height="50dp"
                                android:src="@drawable/wamod_brian_"
                                android:id="@+id/wamod_drawer_photo"/>
                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerVertical="true"
                                android:layout_toRightOf="@id/wamod_drawer_photo"
                                android:orientation="vertical"
                                android:layout_marginLeft="7dp">
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Lorem ipsum"
                                    android:textColor="#ffffff"
                                    android:textStyle="bold"
                                    android:textSize="14sp"
                                    android:id="@+id/wamod_drawer_usernametv"/>
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="dolor sit amet"
                                    android:textColor="#ffffff"
                                    android:id="@+id/wamod_drawer_usernumbertv"
                                    android:textSize="14sp"
                                    android:layout_marginTop="1dp"/>
                            </LinearLayout>
                        </RelativeLayout>
                        <!--<ImageButton
                            android:layout_width="?actionBarSize"
                            android:layout_height="?actionBarSize"
                            android:src="?homeAsUpIndicator"
                            android:background="?selectableItemBackground"
                            android:clickable="true"
                            android:tint="#ffffff"
                            android:id="@+id/wamod_drawer_back"/>-->
                    </RelativeLayout>
                    <ScrollView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_below="@id/wamod_drawer_header">
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical"
                            android:paddingTop="5dp"
                            android:id="@+id/wamod_drawer_buttons">
                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="48dp"
                                android:background="?selectableItemBackground"
                                android:clickable="true"
                                android:id="@+id/wamod_drawer_newgroup">
                                    <ImageView
                                        android:layout_width="24dp"
                                        android:layout_height="24dp"
                                        android:src="@drawable/wamod_drawer_ic_newgroup"
                                        android:layout_centerVertical="true"
                                        android:layout_marginLeft="20dp"
                                        android:padding="2dp"/>
                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:textColor="#ffffff"
                                        android:text="@string/new_group"
                                        android:textSize="15sp"
                                        android:layout_centerVertical="true"
                                        android:layout_marginLeft="72dp" />
                            </RelativeLayout>
                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="48dp"
                                android:background="?selectableItemBackground"
                                android:clickable="true"
                                android:id="@+id/wamod_drawer_wamodweb">
                                <ImageView
                                    android:layout_width="24dp"
                                    android:layout_height="24dp"
                                    android:src="@drawable/wamod_drawer_ic_wamodweb"
                                    android:layout_centerVertical="true"
                                    android:layout_marginLeft="20dp"
                                    android:padding="2dp"/>
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textColor="#ffffff"
                                    android:text="@string/whatsapp_web"
                                    android:textSize="15sp"
                                    android:layout_centerVertical="true"
                                    android:layout_marginLeft="72dp"/>
                            </RelativeLayout>
                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="48dp"
                                android:background="?selectableItemBackground"
                                android:clickable="true"
                                android:id="@+id/wamod_drawer_setstatus">
                                <ImageView
                                    android:layout_width="24dp"
                                    android:layout_height="24dp"
                                    android:src="@drawable/wamod_drawer_ic_status"
                                    android:layout_centerVertical="true"
                                    android:layout_marginLeft="20dp"
                                    android:padding="2dp"/>
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textColor="#ffffff"
                                    android:text="@string/settings_privacy_status"
                                    android:textSize="15sp"
                                    android:layout_centerVertical="true"
                                    android:layout_marginLeft="72dp"/>
                            </RelativeLayout>
                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="48dp"
                                android:background="?selectableItemBackground"
                                android:clickable="true"
                                android:id="@+id/wamod_drawer_changeprofilepic">
                                <ImageView
                                    android:layout_width="24dp"
                                    android:layout_height="24dp"
                                    android:src="@drawable/wamod_drawer_ic_profilepic"
                                    android:layout_centerVertical="true"
                                    android:layout_marginLeft="20dp"
                                    android:padding="2dp"/>
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textColor="#ffffff"
                                    android:text="@string/settings_profile_info"
                                    android:textSize="15sp"
                                    android:layout_centerVertical="true"
                                    android:layout_marginLeft="72dp"/>
                            </RelativeLayout>
                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="48dp"
                                android:background="?selectableItemBackground"
                                android:clickable="true"
                                android:id="@+id/wamod_drawer_starredmessages">
                                <ImageView
                                    android:layout_width="24dp"
                                    android:layout_height="24dp"
                                    android:src="@drawable/wamod_drawer_ic_starredmessages"
                                    android:layout_centerVertical="true"
                                    android:layout_marginLeft="20dp"
                                    android:padding="2dp"/>
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textColor="#ffffff"
                                    android:text="@string/starred_messages"
                                    android:textSize="15sp"
                                    android:layout_centerVertical="true"
                                    android:layout_marginLeft="72dp"/>
                            </RelativeLayout>
                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="48dp"
                                android:background="?selectableItemBackground"
                                android:clickable="true"
                                android:id="@+id/wamod_drawer_search">
                                <ImageView
                                    android:layout_width="24dp"
                                    android:layout_height="24dp"
                                    android:src="@drawable/wamod_drawer_ic_search"
                                    android:layout_centerVertical="true"
                                    android:layout_marginLeft="20dp"
                                    android:padding="2dp"/>
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textColor="#ffffff"
                                    android:text="@string/search"
                                    android:textSize="15sp"
                                    android:layout_centerVertical="true"
                                    android:layout_marginLeft="72dp"/>
                            </RelativeLayout>
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:background="#30ffffff"
                                android:layout_marginLeft="32dp"
                                android:layout_marginRight="32dp"
                                android:layout_marginTop="10dp"
                                android:layout_marginBottom="10dp"
                                android:clickable="true" />
                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="48dp"
                                android:background="?selectableItemBackground"
                                android:clickable="true"
                                android:id="@+id/wamod_drawer_settings">
                                <ImageView
                                    android:layout_width="24dp"
                                    android:layout_height="24dp"
                                    android:src="@drawable/wamod_drawer_ic_settings"
                                    android:layout_centerVertical="true"
                                    android:layout_marginLeft="20dp"
                                    android:padding="2dp"/>
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textColor="#ffffff"
                                    android:text="@string/settings_general"
                                    android:textSize="15sp"
                                    android:layout_centerVertical="true"
                                    android:layout_marginLeft="72dp"/>
                            </RelativeLayout>
                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="48dp"
                                android:background="?selectableItemBackground"
                                android:clickable="true"
                                android:id="@+id/wamod_drawer_wamodsettings">
                                <ImageView
                                    android:layout_width="24dp"
                                    android:layout_height="24dp"
                                    android:src="@drawable/wamod_drawer_ic_wamodsettings"
                                    android:layout_centerVertical="true"
                                    android:layout_marginLeft="20dp"
                                    android:padding="2dp"/>
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:textColor="#ffffff"
                                    android:text="@string/wamod_wamodsettings"
                                    android:textSize="15sp"
                                    android:layout_centerVertical="true"
                                    android:layout_marginLeft="72dp"/>
                            </RelativeLayout>
                        </LinearLayout>
                    </ScrollView>
                </RelativeLayout>
            </RelativeLayout>
    <!--</com.wamod.NavigationDrawer>
    </LinearLayout>-->
</merge>